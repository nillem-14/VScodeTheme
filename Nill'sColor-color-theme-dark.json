	  "tokenColors": [
		{
		  "scope": [
			"variable",
			"string constant.other.placeholder",
			"invalid.deprecated",
			"keyword",
		  ],
		  "settings": {
			"foreground": "#62d2ffe3"
		  }
		},
		{
